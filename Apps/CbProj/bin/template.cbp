<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="external_[ProjectName]" />
		<Option pch_mode="2" />
		<Option compiler="cygwin_gcc__win_emu" />
		<Build>
			<Target title="Debug">
				<Option output="$(OUTBIN)\[ProjectName]" prefix_auto="1" extension_auto="1" />
				<Option object_output="$(OUTOBJ)\[ProjectName]\" />
				<Option type="3" />
				<Option compiler="cygwin_gcc__win_emu" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
			</Target>
		</Build>
		<Compiler>
			<Add option="-include [ProjectName]_forceinc.h" />
			<Add directory="$(CYGWINPATH)\usr\include" />
		</Compiler>
[SourceFiles]
		<Unit filename="[ProjectName]_forceinc.h" />
		<Extensions>
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
